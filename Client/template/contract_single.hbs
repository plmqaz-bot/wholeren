<tr name="{{id}}">
        <td>{{shortText client/chineseName}}</td>
        
        
        
        <td>{{contractCategory/contractCategory}}</td>
        <td>{{createdAt}}</td>
        <td>{{lead/lead}}</td>
        <td>{{shortText leadName}}</td>
        <td>{{leadLevel.leadLevel}}</td>
        
        <td data-attr="assistant" data-restri="{'role':'销售'}" data-coll="User" data-optext="nickname" class="selectbox" data-model="Contract">{{assistant/nickname}}</td>
        <td data-attr="assisCont" data-restri="{'role':'销售'}" data-coll="User" data-optext="nickname" class="selectbox" data-model="Contract">{{assisCont/nickname}}</td>
        <td data-attr="sales" data-restri="{'role':'销售'}" data-coll="User" data-optext="nickname" class="selectbox" data-model="Contract">{{sales/nickname}}</td>
        <td data-attr="expert" data-restri="{'role':'销售'}" data-coll="User" data-optext="nickname" class="selectbox" data-model="Contract">{{expert/nickname}}</td>
        <td>{{status.status}}</td>    
        <td data-attr="salesFollowup"  class="textbox" data-model="Contract">{{shortText salesFollowup}}</td>
        <td data-attr="salesRecord"  class="textbox" data-model="Contract">{{shortText salesRecord}}</td>
        <td>{{expertContactDate}}</td>
        <td data-attr="expertFollowup"  class="textbox" data-model="Contract">{{shortText expertFollowup}}</td>
        <td>{{client/firstName}}</td>
        <td>{{client/lastName}}</td>
        <td data-attr="originalText"  class="textbox" data-model="Contract">{{shortText originalText}}</td>  
        <td>{{shortText client/primaryEmail}}</td>
        <td>{{shortText client/primaryPhone}}</td>
        <td>{{client/otherInfo}}</td>
        <td>{{country.country}}</td>
        <td>{{displayBool validI20}}</td>
        <td>{{shortText previousSchool}}</td>
        <td>{{shortText targetSchool}}</td>
        <td>{{gpa}}</td>
        <td>{{toefl}}</td>
        <td>{{otherScore}}</td>
        <td>{{age}}</td>
        <td>{{degree/degree}}</td>
        <td>{{major}}</td>
        <td data-attr="diagnose"  class="textbox" data-model="Contract">{{shortText diagnose}}</td>
        <td>{{contractSigned}}</td>
        <td>{{contractPaid}}</td>
        <td data-attr="service"  data-coll="ServiceType" data-optext="serviceType" class="multiselectbox" data-model="Contract">{{#each service}}{{#ifCond serviceType/category "Transfer"}}<a href="/admin/service/{{id}}">{{serviceType/serviceType}}</a>{{/ifCond}}{{/each}}</td>
        <td data-attr="service"  data-coll="ServiceType" data-optext="serviceType" class="multiselectbox" data-model="Contract">{{#each service}}{{#ifCond serviceType/category "Emerg"}}<a href="/admin/service/{{id}}">{{serviceType/serviceType}}</a>{{/ifCond}}{{/each}}</td>
        <td data-attr="service"  data-coll="ServiceType" data-optext="serviceType" class="multiselectbox" data-model="Contract">{{#each service}}{{#ifCond serviceType/category "Study"}}<a href="/admin/service/{{id}}">{{serviceType/serviceType}}</a>{{/ifCond}}{{/each}}</td>
        <td data-attr="service"  data-coll="ServiceType" data-optext="serviceType" class="multiselectbox" data-model="Contract">{{#each service}}{{#ifCond serviceType/category "Visa"}}<a href="/admin/service/{{id}}">{{serviceType/serviceType}}</a>{{/ifCond}}{{/each}}</td>
        <td>{{contractPrice}}</td>
        <td>{{shortText contractDetail}}</td> 
        <td>{{displayBool applicationFeePaid}}</td>
        <td>{{paymentOption/paymentOption}}</td>
        <td>{{displayBool endFeeDue}}</td>
        <td>{{displayBool endFee}}</td>
        <td data-attr="teacher" data-restri="{'role':'申请'}" data-coll="User" data-optext="nickname" class="selectbox" data-model="Contract">{{teacher/nickname}}</td>
        <td><a class="comment_edit" href="#{{id}}">Show Comments</a></td>
        <td><a class="edit" href="#{{id}}">Edit</a>{{#if displayDelete}}<a class="del" href="#{{id}}">Delete</a>{{/if}}</td>
        </tr> 